@using System.Collections
@model RunningObjects.MVC.Member
@{
    var count = 0;
    if (Model.Value != null)
    {
        var items = Model.Value as IEnumerable;
        if(items != null)
        {
            count = Queryable.Count((dynamic) items.AsQueryable());
        }
    }
    <text>@count</text>
}
